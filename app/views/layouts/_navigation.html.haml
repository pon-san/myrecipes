%nav.navbar.navbar-expand-lg.navbar-light.bg-light
  .navbar-brand= link_to "Navbar", "#"
  %button.navbar-toggler{type: "button", data_toggle: "collapse", data_target: "#navbarSupportedContent", aria_controls: "navbarSupportedContent", aria_expanded: "false", aria_label: "Toggle navigation"}
    %span.navbar-toggler-icon

  .collapse.navbar-collapse#navbarSupportedContent
    %ul.navbar-nav.mr-auto
      %li.nav-item.active
        .nav-link= link_to "Home", "#"
      %li.nav-item
        .nav-link= link_to "Link", "#"
      %li.nav-item.dropdown
        #navbarDropdown.nav-link.dropdown-toggle{role: "button", data_toggle: "dropdown", aria_haspopup: "true", aria_expanded: "false"}
          = link_to "Dropdown", "#"
        .dropdown-menu{aria_labelledby: "navbarDropdown"}
          .dropdown-item= link_to "Action", "#"
          .dropdown-item= link_to "Action", "#"
          .dropdown-divider
          .dropdown-item= link_to "somethingelse", "#"
      %li.nav-item
        .nav-link.disabled= link_to "Disabled", "#"
    %form.form-inline.my-2.my-lg-0
      %input.form-control.mr-sm-2{type: "search", placeholder: "Search", aria_label: "Search"}
      %button.btn.btn-outline-success.my-2.my-sm-0{type: "submit"}Search